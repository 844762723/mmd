<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mmt.dao.TaskDao">
    <select id="getLastSyncDate" resultType="map">
        select DATE_FORMAT(date, '%Y-%m-%d %T') date from sync_accesstory where state = 1 order by date desc limit 1
    </select>
    <select id="getBookAccs" resultType="accessory">
        select * from accessory
    </select>
    <select id="getHeadiconList" resultType="user">
        select headicon from user
    </select>
</mapper>